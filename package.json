{
  "name": "objectify-folder",
  "version": "0.0.0-development",
  "description": "turn all non index.js modules in a folder as a single object",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-js && npm run test-mjs",
    "test-js": "mocha -R spec --recursive",
    "test-mjs": "DEBUG=objectify-folder node --experimental-modules test-mjs/test.mjs",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "folder",
    "modules"
  ],
  "author": "mateodelnorte",
  "license": "ISC",
  "homepage": "https://github.com/CloudNativeEntrepreneur/objectify-folder",
  "dependencies": {
    "debug": "4.3.4",
    "glob": "7.2.3"
  },
  "devDependencies": {
    "assert": "2.0.0",
    "mocha": "9.2.0",
    "should": "13.2.3",
    "semantic-release": "19.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudNativeEntrepreneur/objectify-folder.git"
  }
}
